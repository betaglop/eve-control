{
  "name": "fr.librinfo.evecontrol",
  "version": "2.0.0",
  "displayName": "Eve-control",
  "cordova": {
    "platforms": [
      "android",
      "browser"
    ],
    "plugins": {
      "cordova-plugin-whitelist": {},
      "cordova-plugin-nativestorage": {},
      "cordova-plugin-network-information": {},
      "cordova-plugin-media": {},
      "cordova-plugin-insomnia": {},
      "cordova-plugin-keyboard": {},
      "com.bez4pieci.cookies": {},
      "cordova-plugin-android-volume": {},
      "cordova-plugin-crosswalk-webview": {
        "XWALK_VERSION": "23+",
        "XWALK_LITEVERSION": "xwalk_core_library_canary:17+",
        "XWALK_COMMANDLINE": "--disable-pull-to-refresh-effect",
        "XWALK_MODE": "embedded",
        "XWALK_MULTIPLEAPK": "true"
      },
      "cordova-plugin-crosswalk-certificate": {}
    }
  },
  "dependencies": {
    "com.bez4pieci.cookies": "https://github.com/bez4pieci/Phonegap-Cookies-Plugin.git",
    "cordova-android": "^6.2.3",
    "cordova-browser": "^5.0.1",
    "cordova-plugin-android-volume": "0.0.6",
    "cordova-plugin-compat": "^1.1.0",
    "cordova-plugin-crosswalk-certificate": "git+https://github.com/danjarvis/cordova-plugin-crosswalk-certificate.git",
    "cordova-plugin-crosswalk-webview": "^2.3.0",
    "cordova-plugin-file": "^4.3.3",
    "cordova-plugin-insomnia": "git+https://github.com/EddyVerbruggen/Insomnia-PhoneGap-Plugin.git",
    "cordova-plugin-keyboard": "^1.1.5",
    "cordova-plugin-media": "^3.0.1",
    "cordova-plugin-nativestorage": "^2.2.2",
    "cordova-plugin-network-information": "^1.3.3",
    "cordova-plugin-whitelist": "^1.3.2",
    "cordova-remove-cookies": "git+https://github.com/bez4pieci/Phonegap-Cookies-Plugin.git"
  }
}