<?xml version='1.0' encoding='utf-8'?>
<widget id="fr.librinfo.evecontrol2" version="2.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>Eve-control</name>
    <description>
        Control your e-venement tickets from your android device
    </description>
    <author email="romain.sanchez@libre-informatique.fr">
        Romain Sanchez
    </author>
    <content src="index.html" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <access origin="*" />
    <allow-navigation href="*" />
</widget>
